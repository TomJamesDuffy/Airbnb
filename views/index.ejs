<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
<link rel="stylesheet" href="index.css">
<div class="center">
<% include partials/header%>
<h1 style="color:White;"> Welcome to Nebula</h1>
<form method="GET" action="/property/new">
<input type = "submit" value="Add Property">
</form>
<ul>
<% user.availableProperties().forEach(function(property){ %>
  <div>
    <li><h3><%= property._details.name  %>, <%= property._details.dates  %>, <%= property._details.price  %>, <%= property._details.desc  %> </h3></li>
    <img src="<%= property._details.photo  %>" style="height: 200px; width: 200px;">
  </div>
<% }) %>
</ul>
<% include partials/footer%>
</div>
